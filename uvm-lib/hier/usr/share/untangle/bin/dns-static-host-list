#!/bin/dash

exec 2> /dev/null

## This should print all of the Dns Static Hosts in the form [<IP Address>;<Hostname>[ <Hostname>]*\n]*
DNS_MASQ_HOSTS=/etc/untangle-net-alpaca/dnsmasq-hosts

if [ -f ${DNS_MASQ_HOSTS} ]; then
    awk '/^[0-9]/ { sub( " ", ";" ) ; print }' ${DNS_MASQ_HOSTS}
fi

exit 0
