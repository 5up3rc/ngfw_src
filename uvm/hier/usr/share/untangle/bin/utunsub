#! /bin/bash

if [ $# -lt 1 ] ; then
    echo "Usage: $0 example-libitem"
    exit 1
fi

FAILED=0
/usr/bin/curl --max-time 30 -o /dev/null "https://poptrack.untangle.com/register/unsub.php?box_id=`cat /usr/share/untangle/activation.key`&filename=\"$1\""  &> /dev/null
if [ $? -ne 0 ] ; then FAILED=1 ; fi

if [ $FAILED -ne 0 ] ; then
    echo "Failed. Rescheduling \"$0 $1\" for +1 hour" &>> /var/log/uvm/apt.log
    echo "$0 $1 &> /dev/null" | at "+1 hour" &> /dev/null
fi

exit 0
