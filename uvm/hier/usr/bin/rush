#!/bin/sh

for f in @PREFIX@/usr/share/untangle/toolbox/* @PREFIX@/usr/share/untangle/lib/untangle-libuvm-{api,reporting}; do
    CLASSPATH=$f:$CLASSPATH
done

OPTS=""

if `tty -s`; then
    OPTS="--tty $OPTS"
fi

export CLASSPATH

if [ -x /usr/bin/jruby1.1 ]; then
  JRUBY=jruby1.1
else
  JRUBY=jruby1.0
fi

$JRUBY @PREFIX@/usr/lib/jruby-ut0.9/lib/ruby/1.8/rush.rb $OPTS "$@"
