<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>MVVM HTTP Invoker</display-name>
  <description>
    RPC over HTTP.
  </description>

  <servlet>
    <servlet-name>HttpInvoker</servlet-name>
    <servlet-class>
      com.untangle.mvvm.servlet.invoker.HttpInvokerServlet
    </servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HttpInvoker</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

  @JSP_PRE_COMPILED_SERVLETS@
</web-app>
