<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false"
>
    <class
        name="com.metavize.mvvm.tran.PipelineStats"
        table="PL_STATS"
        mutable="false"
    >

        <id
            name="id"
            column="EVENT_ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-PipelineStats.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="razeDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="RAZE_DATE"
        />

        <property
            name="c2pBytes"
            type="long"
            update="true"
            insert="true"
            column="C2P_BYTES"
        />

        <property
            name="s2pBytes"
            type="long"
            update="true"
            insert="true"
            column="S2P_BYTES"
        />

        <property
            name="p2cBytes"
            type="long"
            update="true"
            insert="true"
            column="P2C_BYTES"
        />

        <property
            name="p2sBytes"
            type="long"
            update="true"
            insert="true"
            column="P2S_BYTES"
        />

        <property
            name="c2pChunks"
            type="long"
            update="true"
            insert="true"
            column="C2P_CHUNKS"
        />

        <property
            name="s2pChunks"
            type="long"
            update="true"
            insert="true"
            column="S2P_CHUNKS"
        />

        <property
            name="p2cChunks"
            type="long"
            update="true"
            insert="true"
            column="P2C_CHUNKS"
        />

        <property
            name="p2sChunks"
            type="long"
            update="true"
            insert="true"
            column="P2S_CHUNKS"
        />

        <many-to-one
            name="pipelineEndpoints"
            class="com.metavize.mvvm.tran.PipelineEndpoints"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            column="PL_ENDP_ID"
            not-null="true"
        />

        <property
            name="timeStamp"
            type="java.util.Date"
            update="true"
            insert="true"
            column="TIME_STAMP"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PipelineStats.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
