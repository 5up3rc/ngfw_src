<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false"
>
    <class
        name="com.metavize.mvvm.snmp.SnmpSettings"
        table="SNMP_SETTINGS"
    >

        <id
            name="id"
            column="SNMP_SETTINGS_ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SnmpSettings.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="enabled"
            type="boolean"
            update="true"
            insert="true"
            column="ENABLED"
        />

        <property
            name="port"
            type="int"
            update="true"
            insert="true"
            column="PORT"
        />

        <property
            name="communityString"
            type="java.lang.String"
            update="true"
            insert="true"
            column="COM_STR"
        />

        <property
            name="sysContact"
            type="java.lang.String"
            update="true"
            insert="true"
            column="SYS_CONTACT"
        />

        <property
            name="sysLocation"
            type="java.lang.String"
            update="true"
            insert="true"
            column="SYS_LOCATION"
        />

        <property
            name="sendTraps"
            type="boolean"
            update="true"
            insert="true"
            column="SEND_TRAPS"
        />

        <property
            name="trapHost"
            type="java.lang.String"
            update="true"
            insert="true"
            column="TRAP_HOST"
        />

        <property
            name="trapCommunity"
            type="java.lang.String"
            update="true"
            insert="true"
            column="TRAP_COM"
        />

        <property
            name="trapPort"
            type="int"
            update="true"
            insert="true"
            column="TRAP_PORT"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SnmpSettings.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
