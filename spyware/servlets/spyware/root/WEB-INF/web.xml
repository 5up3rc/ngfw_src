<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Spyware WebApp</display-name>

  <servlet>
    <servlet-name>BlockPageServlet</servlet-name>
    <servlet-class>com.untangle.node.spyware.BlockPageServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
 </servlet>

  <servlet>
    <servlet-name>UnblockerServlet</servlet-name>
    <servlet-class>com.untangle.node.spyware.UnblockerServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
 </servlet>

  @JSP_PRE_COMPILED_SERVLETS@


  <servlet-mapping>
    <servlet-name>BlockPageServlet</servlet-name>
    <url-pattern>/blockpage</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>UnblockerServlet</servlet-name>
    <url-pattern>/unblock</url-pattern>
  </servlet-mapping>

</web-app>
