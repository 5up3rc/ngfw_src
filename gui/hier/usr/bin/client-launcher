#!/bin/sh

#
# PROFILING 
#
export PROFILE_GUI_YJP LD_LIBRARY_PATH
PROFILE_GUI_YJP=no
if [ "x$PROFILE_GUI_YJP" = "xyes" ] ; then
	echo "YJP profiling support enabled"
	LD_LIBRARY_PATH=/opt/yjp/bin/linux:$LD_LIBRARY_PATH
	JAVA_OPTS="-agentlib:yjpagent $JAVA_OPTS"
fi

echo "SET JAVA_OPTS"
echo $JAVA_OPTS

#
# CLASSPATH
#
for f in /home/inieves/work/alpine/output/usr/share/untangle/web/webstart/*jar ; do
    CLASSPATH="$CLASSPATH:$f"
done
for f in /home/inieves/work/alpine/output/usr/share/untangle/web/webstart/*mar ; do
    CLASSPATH="$CLASSPATH:$f"
done
export CLASSPATH

echo "SET CLASSPATH"
echo $CLASSPATH


exec /usr/lib/jvm/java-1.5.0-sun/bin/java -verbose:class ${JAVA_OPTS} com.untangle.gui.util.MLauncher
