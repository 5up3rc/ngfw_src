    <parameter name="endTime" isForPrompting="true" class="java.sql.Timestamp"/>
    <queryString><![CDATA[
SELECT rule_sid,
       blocked,
       COUNT(rule_sid) AS TOTAL_ACTIONS,
       classification,
       message AS DESCRIPTION
  FROM n_ips_evt
  WHERE time_stamp >= $P{startTime}
    AND time_stamp < $P{endTime}
  GROUP BY rule_sid, blocked, classification, message
  ORDER BY TOTAL_ACTIONS DESC
  LIMIT 15
         ]]></queryString>
