	<parameter name="startTime" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="endTime" isForPrompting="true" class="java.sql.Timestamp"/>


	<queryString><![CDATA[SELECT evt.time_stamp as TIME_STAMP, 
				     is_traffic_blocker as ACTION,
				     rule_index as REASON,
					CASE WHEN proto=1 THEN 'PING'
						WHEN proto=6 THEN 'TCP'
						WHEN proto=17 THEN 'UDP'
					END as PROTOCOL,
				     c_client_addr as SRC_IP,
				     c_client_port as SRC_PORT,
				     c_server_addr as DST_IP,
				     c_server_port as DST_PORT
FROM tr_firewall_evt evt
JOIN pl_endp USING (session_id)
JOIN firewall_rule USING (rule_id)
WHERE evt.time_stamp > $P{startTime} AND evt.time_stamp < $P{endTime}
ORDER BY evt.time_stamp DESC]]></queryString>

