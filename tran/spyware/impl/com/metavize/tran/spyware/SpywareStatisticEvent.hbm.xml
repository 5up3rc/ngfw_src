<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false"
>
    <class
        name="com.metavize.tran.spyware.SpywareStatisticEvent"
        table="TR_SPYWARE_STATISTIC_EVT"
        mutable="false"
    >

        <id
            name="id"
            column="EVENT_ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SpywareStatisticEvent.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="pass"
            type="int"
            update="true"
            insert="true"
            column="PASS"
        />

        <property
            name="cookie"
            type="int"
            update="true"
            insert="true"
            column="COOKIE"
        />

        <property
            name="activeX"
            type="int"
            update="true"
            insert="true"
            column="ACTIVEX"
        />

        <property
            name="URL"
            type="int"
            update="true"
            insert="true"
            column="URL"
        />

        <property
            name="subnetAccess"
            type="int"
            update="true"
            insert="true"
            column="SUBNET_ACCESS"
        />

        <property
            name="timeStamp"
            type="java.util.Date"
            update="true"
            insert="true"
            column="TIME_STAMP"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpywareStatisticEvent.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
