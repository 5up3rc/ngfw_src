<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false"
>
    <class
        name="com.metavize.tran.spyware.SpywareAccessEvent"
        table="TR_SPYWARE_EVT_ACCESS"
        mutable="false"
    >

        <id
            name="id"
            column="EVENT_ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SpywareAccessEvent.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="pipelineEndpoints"
            class="com.metavize.mvvm.tran.PipelineEndpoints"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            column="PL_ENDP_ID"
            not-null="true"
        />

        <property
            name="ipMaddr"
            type="com.metavize.mvvm.type.IPMaddrUserType"
            update="true"
            insert="true"
        >
            <column
                name="IPMADDR" 
                sql-type="inet"
            />
        </property>

        <property
            name="identification"
            type="java.lang.String"
            update="true"
            insert="true"
            column="IDENT"
        />

        <property
            name="blocked"
            type="boolean"
            update="true"
            insert="true"
            column="BLOCKED"
        />

        <property
            name="timeStamp"
            type="java.util.Date"
            update="true"
            insert="true"
            column="TIME_STAMP"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpywareAccessEvent.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
