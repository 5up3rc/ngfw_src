<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>HttpBlocker WebApp</display-name>

  <servlet>
    <servlet-name>UnblockerServlet</servlet-name>
    <servlet-class>com.metavize.tran.httpblocker.UnblockerServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
 </servlet>

  @JSP_PRE_COMPILED_SERVLETS@

  <servlet-mapping>
    <servlet-name>UnblockerServlet</servlet-name>
    <url-pattern>/unblock</url-pattern>
  </servlet-mapping>

</web-app>
