	<parameter name="startTime" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="endTime" isForPrompting="true" class="java.sql.Timestamp"/>
	<queryString><![CDATA[
SELECT COALESCE(NULLIF(name, ''), HOST(client_addr)) AS HNAME,
       SUM(dropped) AS DROPS, SUM(rejected) AS REJECTS, SUM(limited) AS LIMITS,
       (SUM(dropped) + SUM(rejected) + SUM(limited)) AS TOTAL_ACTIONS
  FROM shield_rejection_evt
         LEFT OUTER JOIN merged_address_map
           ON (client_addr = addr
             AND time_stamp >= start_time
             AND time_stamp < end_time)
  WHERE time_stamp >= $P{startTime}
    AND time_stamp < $P{endTime}
  GROUP BY HNAME
  ORDER BY TOTAL_ACTIONS DESC, HNAME ASC
  LIMIT 15
         ]]></queryString>
