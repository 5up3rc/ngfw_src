	<queryString><![CDATA[
SELECT rule_index AS RULE,
       SUM(p2c_bytes) + SUM(c2p_bytes) AS DATA_TRANSFERRED
  FROM tr_nat_redirect_evt
         JOIN pl_endp USING (session_id)
         JOIN pl_stats USING (session_id)
  WHERE time_stamp >= $P{startTime} AND time_stamp < $P{endTime} AND NOT is_dmz
  GROUP BY rule_id, RULE
  ORDER BY DATA_TRANSFERRED DESC, RULE ASC
  LIMIT 15
         ]]></queryString>
